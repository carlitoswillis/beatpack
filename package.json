{
  "name": "beatpack",
  "version": "1.0.0",
  "description": "zip, convert to mp3, create a video, and upload to YouTube",
  "main": "main.js",
  "scripts": {
    "build:dev": "parallel-webpack --watch",
    "test": "jest",
    "test:dev": "jest --watchAll",
    "cli": "node index.js",
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"BeatPack\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "author": "Carlitos Willis",
  "license": "ISC",
  "dependencies": {
    "archiver": "^5.0.0",
    "custom-electron-titlebar": "^3.2.3",
    "detect-node": "^2.0.4",
    "dragula": "^3.7.2",
    "ejs": "^3.1.3",
    "electron": "^9.1.2",
    "electron-packager": "^15.0.0",
    "electron-reload": "^1.5.0",
    "electron-tabs": "^0.15.0",
    "express": "^4.17.1",
    "fluent-ffmpeg": "^2.1.2",
    "gm": "^1.23.1",
    "google-auth-library": "^6.0.6",
    "googleapis": "^58.0.0",
    "image-size": "^0.8.3",
    "jquery": "^3.5.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "node-lame": "^1.2.2",
    "nodemon": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-transition-group": "^4.4.1",
    "uninstall": "0.0.0",
    "webpack-cli": "^3.3.12",
    "xmlhttprequest": "^1.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.0",
    "jest": "^26.2.2",
    "parallel-webpack": "^2.6.0",
    "svg-inline-loader": "^0.8.2",
    "webpack": "^4.44.0"
  }
}
