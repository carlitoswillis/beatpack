{
  "name": "beatpack",
  "version": "1.0.0",
  "description": "zip up stems, convert main file to mp3, create a video from the beat",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "cli": "node index.js",
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"BeatPack\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "author": "Carlitos Willis",
  "license": "ISC",
  "dependencies": {
    "archiver": "^5.0.0",
    "electron": "^9.1.1",
    "electron-packager": "^15.0.0",
    "electron-reload": "^1.5.0",
    "fluent-ffmpeg": "^2.1.2",
    "jquery": "^3.5.1",
    "node-lame": "^1.2.2",
    "xmlhttprequest": "^1.8.0"
  }
}
